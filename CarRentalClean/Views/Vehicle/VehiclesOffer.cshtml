@using CarRentalSystem.Core.Entities;
@model IEnumerable<Vehicle>


<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Vehicle model</th>
            <th scope="col">Color</th>
            <th scope="col">Price per day</th>
            <th scope="col">Number plate</th>


        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <th scope="row">@obj.Id</th>
                <td>@obj.Model</td>
                <td>
                   @obj.Color
                </td>
                <td>@obj.PricePerDay</td>
                <td>
                    @obj.PlateNumber
                </td>
               <td>

                  
                        <a asp-controller="Vehicle" asp-action="PublishOffer" asp-route-id="@obj.Id">Approve</a>
                    

                </td>
            </tr>
        }

    </tbody>
</table>
